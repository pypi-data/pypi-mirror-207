                                                      owl:onProperty syn:code ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange syn:snomed:SNOMED-CT
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty syn:reasonCode ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange syn:snomed:SNOMED-CT
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty syn:reasonCode ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
