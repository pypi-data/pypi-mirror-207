<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <!-- interface-name stickytext.ui -->
  <!-- interface-description Main Window for stickytext -->
  <!-- interface-authors BeatLink -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkApplicationWindow" id="window">
    <property name="accessible-role">window</property>
    <property name="child">scrolledwindow</property>
    <property name="default-height">200</property>
    <property name="default-width">200</property>
    <property name="height-request">200</property>
    <property name="startup-id">StickyText</property>
    <property name="title">StickyText</property>
    <property name="width-request">200</property>
    <child>
      <object class="GtkScrolledWindow" id="scrolledwindow">
        <child>
          <object class="GtkTextView" id="textview">
            <property name="accessible-role">text-box</property>
            <property name="bottom-margin">12</property>
            <property name="buffer">textbuffer</property>
            <property name="hexpand">True</property>
            <property name="hscroll-policy">natural</property>
            <property name="left-margin">12</property>
            <property name="right-margin">12</property>
            <property name="top-margin">12</property>
            <property name="vexpand">True</property>
            <property name="vscroll-policy">natural</property>
            <property name="wrap-mode">word</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkTextBuffer" id="textbuffer"/>
</interface>
