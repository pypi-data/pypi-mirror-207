graft texonomy

include LICENSE
include README.md
include *.md

include Makefile

prune .github
exclude .gitignore
prune docs

recursive-include texonomy *.tex
recursive-include examples *.pdf

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
global-exclude .DS_Store
